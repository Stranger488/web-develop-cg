{
  "name": "web-develop-cg",
  "version": "1.0.0",
  "description": "IFK-training",
  "main": "index.js",
  "scripts": {
    "start": "gulp",
    "test": "echo \"Error: no test specified\" && exit 1",
    "prettier": "prettier --config .prettierrc.json --write '{static,js,less}/**/*.{js,html,less}'",
    "build": "gulp build",
    "clear-gulp-cache": "gulp cache:clear",
    "deploy:prod": "now --prod"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Stranger488/web-develop-cg.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Stranger488/web-develop-cg/issues"
  },
  "homepage": "https://github.com/Stranger488/web-develop-cg#readme",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@commitlint/cli": "^8.3.4",
    "autoprefixer": "^9.7.4",
    "babelify": "^10.0.0",
    "browser-sync": "^2.26.7",
    "browserify": "^16.5.0",
    "commitizen": "^4.0.3",
    "cz-customizable": "^6.2.0",
    "gulp": "^4.0.2",
    "gulp-autoprefixer": "^7.0.1",
    "gulp-cache": "^1.1.3",
    "gulp-clean-css": "^4.2.0",
    "gulp-imagemin": "^7.1.0",
    "gulp-less": "^4.0.1",
    "gulp-rename": "^2.0.0",
    "gulp-rigger": "^0.5.8",
    "gulp-sourcemaps": "^2.6.5",
    "gulp-uglify": "^3.0.2",
    "husky": "^3.1.0",
    "imagemin-pngquant": "^8.0.0",
    "lint-staged": "^9.5.0",
    "postcss-cli": "^7.1.0",
    "prettier": "^1.19.1",
    "vinyl-buffer": "^1.0.1",
    "vinyl-source-stream": "^2.0.0"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g '.config/git/commitlint.js'",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "{static,js,less}/**/*.{js,html,less}": [
      "npm run prettier",
      "git add"
    ]
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-customizable"
    },
    "cz-customizable": {
      "config": ".config/git/commitizen.js"
    }
  },
  "dependencies": {}
}
